{
  "name": "@form8ion/commitlint-config",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/commitlint-config",
  "bugs": "https://github.com/form8ion/commitlint-config/issues",
  "homepage": "https://npm.im/@form8ion/commitlint-config",
  "sideEffects": false,
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-conventional": "18.6.0"
  },
  "devDependencies": {
    "@form8ion/remark-lint-preset": "6.0.2",
    "@travi/any": "3.0.2",
    "ban-sensitive-files": "1.10.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.7",
    "npm-run-all2": "6.1.1",
    "publint": "0.2.7",
    "remark-cli": "12.0.0"
  }
}
