{
  "name": "@form8ion/commitlint-config",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "repository": "form8ion/commitlint-config",
  "bugs": "https://github.com/form8ion/commitlint-config/issues",
  "homepage": "https://npm.im/@form8ion/commitlint-config",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:travis": "travis-lint .travis.yml"
  },
  "devDependencies": {
    "@form8ion/remark-lint-preset": "1.0.9",
    "@travi/any": "2.0.10",
    "ban-sensitive-files": "1.9.7",
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "npm-run-all": "4.1.5",
    "remark-cli": "8.0.0",
    "travis-lint": "1.0.0"
  },
  "dependencies": {
    "commitlint-config-travi": "1.3.8"
  }
}
