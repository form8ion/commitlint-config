{
  "name": "@form8ion/commitlint-config",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "form8ion/commitlint-config",
  "bugs": "https://github.com/form8ion/commitlint-config/issues",
  "homepage": "https://npm.im/@form8ion/commitlint-config",
  "sideEffects": false,
  "main": "./index.js",
  "exports": "./index.js",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1"
  },
  "devDependencies": {
    "@form8ion/remark-lint-preset": "6.0.7",
    "@travi/any": "3.1.2",
    "ban-sensitive-files": "1.10.7",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "npm-run-all2": "7.0.2",
    "publint": "0.3.7",
    "remark-cli": "12.0.1"
  }
}
